local function RBKJDDZ_fake_script() -- tracers.LocalScript 
	local script = Instance.new('LocalScript', tracers)

	local tracers = script.Parent
	
	local lplr = game.Players.LocalPlayer
	local camera = game:GetService("Workspace").CurrentCamera
	local CurrentCamera = workspace.CurrentCamera
	local worldToViewportPoint = CurrentCamera.worldToViewportPoint
	
	_G.TeamCheck = false -- Use True or False to toggle TeamCheck
	
	local aiming = false
	local connections = {}
	
	local function getTracer(v)
		local Tracer = Drawing.new("Line")
		Tracer.Visible = false
		Tracer.Color = Color3.new(1, 1, 1)
		Tracer.Thickness = 1
		Tracer.Transparency = 1
		Tracer.Name = v.Name .. "_Tracer" -- Identificación
		return Tracer
	end
	
	local function startAiming()
		for _, v in pairs(game.Players:GetPlayers()) do
			if v ~= lplr then
				local Tracer = getTracer(v)
				local connection = game:GetService("RunService").RenderStepped:Connect(function()
					if aiming and v.Character and v.Character:FindFirstChild("Humanoid") and v.Character:FindFirstChild("HumanoidRootPart") and v.Character.Humanoid.Health > 0 then
						local Vector, OnScreen = camera:worldToViewportPoint(v.Character.HumanoidRootPart.Position)
						if OnScreen then
							Tracer.From = Vector2.new(camera.ViewportSize.X / 2, camera.ViewportSize.Y / 1)
							Tracer.To = Vector2.new(Vector.X, Vector.Y)
							if _G.TeamCheck and v.TeamColor == lplr.TeamColor then
								Tracer.Visible = false
							else
								Tracer.Visible = true
							end
						else
							Tracer.Visible = false
						end
					else
						Tracer.Visible = false
					end
				end)
				table.insert(connections, connection)
			end
		end
	
		game.Players.PlayerAdded:Connect(function(v)
			if v ~= lplr then
				local Tracer = getTracer(v)
				local connection = game:GetService("RunService").RenderStepped:Connect(function()
					if aiming and v.Character and v.Character:FindFirstChild("Humanoid") and v.Character:FindFirstChild("HumanoidRootPart") and v.Character.Humanoid.Health > 0 then
						local Vector, OnScreen = camera:worldToViewportPoint(v.Character.HumanoidRootPart.Position)
						if OnScreen then
							Tracer.From = Vector2.new(camera.ViewportSize.X / 2, camera.ViewportSize.Y / 1)
							Tracer.To = Vector2.new(Vector.X, Vector.Y)
							if _G.TeamCheck and v.TeamColor == lplr.TeamColor then
								Tracer.Visible = false
							else
								Tracer.Visible = true
							end
						else
							Tracer.Visible = false
						end
					else
						Tracer.Visible = false
					end
				end)
				table.insert(connections, connection)
			end
		end)
	end
	
	local function stopAiming()
		for _, connection in pairs(connections) do
			connection:Disconnect()
		end
		connections = {}
	end
	
	local function toggleAiming()
		aiming = not aiming
		if aiming then
			tracers.Text = "Off"
			startAiming()
		else
			tracers.Text = "On"
			stopAiming()
		end
	end
	
	tracers.MouseButton1Click:Connect(toggleAiming)
	tracers.Text = "Off" -- Initial button text
	
end
