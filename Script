coroutine.wrap(ZCXUI_fake_script)()
local function VEAPPQ_fake_script() -- ScreenGui.LocalScript 
	local script = Instance.new('LocalScript', ScreenGui)

	local frame = script.Parent:WaitForChild("Frame")
	local TweenService = game:GetService("TweenService")
	
	-- Asegurarse de que el Frame sea visible
	frame.Visible = true
	
	-- Configuración inicial del Frame (fuera de la pantalla)
	frame.Position = UDim2.new(0.5, -frame.Size.X.Offset / 2, 1, 0)  -- Justo fuera de la pantalla inferior
	
	-- Configuración del objetivo de la animación (en pantalla)
	local endPosition = UDim2.new(0.5, -frame.Size.X.Offset / 2, 0.5, -frame.Size.Y.Offset / 2) -- Centro de la pantalla
	
	-- Configuración de Tween (animación)
	local tweenInfo = TweenInfo.new(
		1, -- Tiempo de la animación en segundos
		Enum.EasingStyle.Quad, -- Estilo de la animación
		Enum.EasingDirection.Out, -- Dirección de la animación
		0, -- Número de repeticiones
		false, -- Reverso
		0 -- Retardo
	)
	
	local tween = TweenService:Create(frame, tweenInfo, {Position = endPosition})
	
	-- Esperar un corto período para asegurarse de que el juego ha cargado completamente
	wait(1)
	
	-- Iniciar la animación
	tween:Play()
	
end
